<h1>User management page</h1>
<table>
<table>
    <tr class="font-bold">
        <td>ID</td> 
        <td>Email</td>  
        <td>Edit</td>
    </tr>
<% @user.each do |user| %>
<tr id="<%= dom_id(user) %>">        
    <td><%= user.id %></td><br>
    <td><%= user.email %></td>
    <td><%= link_to 'Edit', edit_admin_user_management_path(user) %></td>
</tr>
<% end %>   
</table>
